{
  "name": "@dnb-org/dnb-hugo-youtube",
  "version": "1.0.1",
  "license": "MIT",
  "description": "Youtube shortcode and partial for GoHugo",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "homepage": "https://github.com/dnb-org/dnb-hugo-youtube",
  "bugs": "https://github.com/dnb-org/dnb-hugo-youtube/issues",
  "devDependencies": {
    "@dnb-org/standard-version-config": "^3.4.10",
    "@dnb-org/tools": "^3.4.12",
    "mem": "^9.0.2"
  },
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release/postrelease.sh",
    "release-major": "standard-version --release-as major -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release/postrelease.sh",
    "release-next": "standard-version --release-as minor -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release/postrelease.sh"
  },
  "private": true
}
